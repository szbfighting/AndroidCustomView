<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InputNumberView">
        <attr name="max" format="integer"/>
        <attr name="min" format="integer"/>
        <attr name="step" format="integer"/>
        <attr name="defaultValue" format="integer"/>
        <attr name="disable" format="boolean"/>
        <attr name="btnBackground" format="reference"/>
        <attr name="valueSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="LoginPageView">
        <attr name="mainColor" format="color"/>
        <attr name="verifyCodeSize" format="integer"/>

    </declare-styleable>

    <declare-styleable name="CustomToolBar">
        <attr name = "titleText" format="string|reference"/>
        <attr name="myTitleTextColor" format="color|reference"/>
        <attr name="titleTextSize" format="dimension|reference"/>
        <attr name="leftImageSrc" format="reference"/>
        <attr name="rightImageSrc" format="reference"/>
    </declare-styleable>
</resources>